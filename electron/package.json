{
  "name": "trace-lab",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "electron-builder": "^26.0.12",
    "wait-on": "^9.0.1"
  },
  "build": {
  "appId": "com.raypauszek.tracelab",
  "mac": {
    "category": "public.app-category.science",
    "target": ["dmg"]
  },
  "files": [
    "main.js",
    "preload.js",
    "package.json"
  ],
  "extraResources": [
    {
      "from": "../backend/dist",
      "to": "backend",
      "filter": ["**/*"]
    },
    {
      "from": "../renderer/dist",
      "to": "renderer",
      "filter": ["**/*"]
    }
  ]
}
}
